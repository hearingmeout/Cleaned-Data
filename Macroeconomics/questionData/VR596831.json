[{"id":"VR596831","type":"mcq","stimulusHtml":"<p class=\"stem_paragraph\">An increase in real income will cause which of the following to occur in the short run?</p>\n","rubricRef":"VR596831_rubric","choicesHtml":["<p class=\"choice_paragraph\">Imports will decrease.</p>","<p class=\"choice_paragraph\">Consumption spending will decrease.</p>","<p class=\"choice_paragraph\">Nominal interest rates will decrease.</p>","<p class=\"choice_paragraph\">The monetary base will increase.</p>","<p class=\"choice_paragraph\">Money demand will increase.</p>"],"answer":4}]