[{"id":"VR187610","type":"mcq","stimulusHtml":"<p class=\"stem_paragraph\">An increase in military spending will affect an economy in which of the following ways in the short run?</p>\n","rubricRef":"VR187610_rubric","choicesHtml":["<p class=\"choice_paragraph\">Aggregate supply will increase.</p>","<p class=\"choice_paragraph\">Aggregate supply will decrease.</p>","<p class=\"choice_paragraph\">Aggregate demand will increase.</p>","<p class=\"choice_paragraph\">Aggregate demand will decrease.</p>","<p class=\"choice_paragraph\">The price level will decrease.</p>"],"answer":2}]