[{"id":"VH916131","type":"mcq","stimulusHtml":"<p class=\"stem_paragraph\">All societies face a trade-off for every decision for which of the following reasons?</p>\n","rubricRef":"VH916131_rubric","choicesHtml":["<p class=\"choice_paragraph\">A decision entails zero opportunity cost.</p>","<p class=\"choice_paragraph\">Resources are scarce.</p>","<p class=\"choice_paragraph\">Some resources are always unemployed.</p>","<p class=\"choice_paragraph\">Resources have no alternative uses.</p>","<p class=\"choice_paragraph\">Resources are not allocated efficiently.</p>"],"answer":1}]