[{"id":"OOQ000001","type":"mcq","stimulusHtml":"<div class=\"qti-layout-row\"> <div class=\"qti-layout-col12\"> <div class=\"prompt\"> <p class=\"dap_body\">Which of the following statements best describes this medication?</p> </div> </div> </div>\n","rubricRef":"OOQ000001_rubric","features":[{"type":"sharedpassage","html":"<html><body><div class=\"stimulus_reference\" xmlns=\"http://www.imsglobal.org/xsd/imsqti_v2p2\" xmlns:html5=\"html5\" xmlns:pci=\"http://www.imsglobal.org/xsd/portableCustomInteraction\" xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\">\n<p class=\"dap_body\">Holly was injured in a car accident. Her doctor prescribed a medication that increases the action of endorphins to decrease pain.</p>\n</div>\n</body></html>"}],"choicesHtml":["<p class=\"dap_body\">It functions as an agonist.</p>","<p class=\"dap_body\">It produces aphasia.</p>","<p class=\"dap_body\">It accelerates sensory interaction.</p>","<p class=\"dap_body\">It acts as an antagonist.</p>"],"answer":"A-98498"}]