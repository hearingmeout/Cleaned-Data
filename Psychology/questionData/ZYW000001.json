[{"id":"ZYW000001","type":"mcq","stimulusHtml":"<div class=\"qti-layout-row\"> <div class=\"qti-layout-col12\"> <div class=\"prompt\"> <p class=\"dap_body\">What principle of perception is being used in this advertisement?</p> </div> </div> </div>\n","rubricRef":"ZYW000001_rubric","features":[{"type":"sharedpassage","html":"<div class=\"stimulus_reference passage-scrolling\" xmlns=\"http://www.imsglobal.org/xsd/imsqti_v2p2\">\n<div class=\"passage\">\n<div class=\"prose\" data-ets-style=\"1\">\n<p class=\"dap_body\">A coffee association wants to increase sales of coffee in one region of the United States. To do so, it puts up billboards on major highways in that region with the following message.</p>\n\n<div class=\"standalone_image qti-align-center\"><img alt=\"DRINK COFFEE. The message uses only capital letters. The strokes of the capital letters are dashed, not solid, lines.\" height=\"199\" src=\"https://assets.learnosity.com/organisations/578/ZYW000000_01.png\" width=\"300\" /></div>\n</div>\n</div>\n</div>\n"}],"choicesHtml":["<p class=\"dap_body\">Linear perspective</p>","<p class=\"dap_body\">Closure</p>","<p class=\"dap_body\">Figure and ground</p>","<p class=\"dap_body\">Similarity</p>"],"answer":"B-81481"}]