[{"id":"MHS000001","type":"mcq","stimulusHtml":"<div class=\"qti-layout-row\"> <div class=\"qti-layout-col12\"> <div class=\"prompt\"> <p class=\"dap_body\">Danny is most likely to be diagnosed with which of the following anxiety disorders?</p> </div> </div> </div>\n","rubricRef":"MHS000001_rubric","features":[{"type":"sharedpassage","html":"<html><body><div class=\"stimulus_reference\" xmlns=\"http://www.imsglobal.org/xsd/imsqti_v2p2\" xmlns:html5=\"html5\" xmlns:pci=\"http://www.imsglobal.org/xsd/portableCustomInteraction\" xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\">\n<p class=\"dap_body\">Danny has an intense fear of snakes. At the sight of a snake, he experiences a powerful fight-flight-freeze response. He avoids all places where he thinks snakes might be.</p>\n</div>\n</body></html>"}],"choicesHtml":["<p class=\"dap_body\">Agoraphobia</p>","<p class=\"dap_body\">Social anxiety disorder</p>","<p class=\"dap_body\">Generalized anxiety disorder</p>","<p class=\"dap_body\">A specific phobia</p>"],"answer":"D-62469"}]