[{"id":"JIG000001","type":"mcq","stimulusHtml":"<div class=\"qti-layout-row\"> <div class=\"qti-layout-col12\"> <div class=\"prompt\"> <p class=\"dap_body\">Which of the following diagnoses is most consistent with Barry’s symptoms?</p> </div> </div> </div>\n","rubricRef":"JIG000001_rubric","features":[{"type":"sharedpassage","html":"<html><body><div class=\"stimulus_reference\" xmlns=\"http://www.imsglobal.org/xsd/imsqti_v2p2\" xmlns:html5=\"html5\" xmlns:pci=\"http://www.imsglobal.org/xsd/portableCustomInteraction\" xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\">\n<p class=\"dap_body\">During a session with a psychologist, 22-year-old Barry reports that he has been seeing things that others cannot see. At one point, he remarks that “his sweatshirt has a headache.” </p>\n</div>\n</body></html>"}],"choicesHtml":["<p class=\"dap_body\">Depressive disorders</p>","<p class=\"dap_body\">Dissociative identity disorder</p>","<p class=\"dap_body\">Obsessive-compulsive disorder</p>","<p class=\"dap_body\">Schizophrenic spectrum disorders</p>"],"answer":"D-98885"}]