[{"id":"VRD000001","type":"mcq","stimulusHtml":"<div class=\"qti-layout-row\"> <div class=\"qti-layout-col12\"> <div class=\"prompt\"> <p class=\"dap_body\">Which of the following concepts does this conversation illustrate?</p> </div> </div> </div>\n","rubricRef":"VRD000001_rubric","features":[{"type":"sharedpassage","html":"<html><body><div class=\"stimulus_reference\" xmlns=\"http://www.imsglobal.org/xsd/imsqti_v2p2\" xmlns:html5=\"html5\" xmlns:pci=\"http://www.imsglobal.org/xsd/portableCustomInteraction\" xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\">\n<p class=\"dap_body\">Four-year-old Ethan and his mother have the following conversation:</p><p class=\"dap_body\">MOTHER: Ethan, do you have a brother?<br/>ETHAN: Yes.<br/>MOTHER: What’s his name?<br/>ETHAN: My brother’s name is Anthony.<br/>MOTHER: Does Anthony have a brother?<br/>ETHAN: No.</p>\n</div>\n</body></html>"}],"choicesHtml":["<p class=\"dap_body\">Conservation&nbsp;</p>","<p class=\"dap_body\">Animism&nbsp;</p>","<p class=\"dap_body\">Object permanence</p>","<p class=\"dap_body\">Egocentrism&nbsp;</p>"],"answer":"D-10900"}]