[{"id":"QWM000001","type":"mcq","stimulusHtml":"<div class=\"qti-layout-row\"> <div class=\"qti-layout-col12\"> <div class=\"prompt\"> <p class=\"dap_body\">Which of the following concepts is most likely to explain Gordon’s thinking?</p> </div> </div> </div>\n","rubricRef":"QWM000001_rubric","features":[{"type":"sharedpassage","html":"<html><body><div class=\"stimulus_reference\" xmlns=\"http://www.imsglobal.org/xsd/imsqti_v2p2\" xmlns:html5=\"html5\" xmlns:pci=\"http://www.imsglobal.org/xsd/portableCustomInteraction\" xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\">\n<p class=\"dap_body\">Gordon notices that his bathroom faucet leaks. He has no experience with plumbing repairs, but he has watched videos online. Therefore, Gordon thinks he can fix the faucet himself.</p>\n</div>\n</body></html>"}],"choicesHtml":["<p class=\"dap_body\">Insight learning</p>","<p class=\"dap_body\">A low level of conscientiousness </p>","<p class=\"dap_body\">An external locus of control</p>","<p class=\"dap_body\">A high level of self-efficacy </p>"],"answer":"D-05458"}]