[{"id":"MWV000001","type":"mcq","stimulusHtml":"<div class=\"qti-layout-row\"> <div class=\"qti-layout-col12\"> <div class=\"prompt\"> <p class=\"dap_body\">Which of the following is most likely developing to cause these changes in Derek’s voice and appearance?</p> </div> </div> </div>\n","rubricRef":"MWV000001_rubric","features":[{"type":"sharedpassage","html":"<html><body><div class=\"stimulus_reference\" xmlns=\"http://www.imsglobal.org/xsd/imsqti_v2p2\" xmlns:html5=\"html5\" xmlns:pci=\"http://www.imsglobal.org/xsd/portableCustomInteraction\" xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\">\n<p class=\"dap_body\">Derek is in seventh grade. Derek’s friends and family have noticed that Derek’s voice is getting deeper, and he has more facial hair.</p>\n</div>\n</body></html>"}],"choicesHtml":["<p class=\"dap_body\">Primary sex characteristics&nbsp;</p>","<p class=\"dap_body\">Egocentrism</p>","<p class=\"dap_body\">Animism</p>","<p class=\"dap_body\">Secondary sex characteristics&nbsp;</p>"],"answer":"D-58831"}]