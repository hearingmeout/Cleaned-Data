[{"id":"KOW000001","type":"mcq","stimulusHtml":"<div class=\"qti-layout-row\"> <div class=\"qti-layout-col12\"> <div class=\"prompt\"> <p class=\"dap_body\">Which of the following psychological phenomena best explains her experience?</p> </div> </div> </div>\n","rubricRef":"KOW000001_rubric","features":[{"type":"sharedpassage","html":"<html><body><div class=\"stimulus_reference\" xmlns=\"http://www.imsglobal.org/xsd/imsqti_v2p2\" xmlns:html5=\"html5\" xmlns:pci=\"http://www.imsglobal.org/xsd/portableCustomInteraction\" xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\">\n<p class=\"dap_body\">Maria wakes up in the middle of the night to get a drink of water. As she enters the kitchen, she sees a bowl of red strawberries that appear brown in the dim light, but she still perceives them to be red.</p>\n</div>\n</body></html>"}],"choicesHtml":["<p class=\"dap_body\">Synesthesia</p>","<p class=\"dap_body\">Top-down processing</p>","<p class=\"dap_body\">Bottom-up processing</p>","<p class=\"dap_body\">Change blindness</p>"],"answer":"B-24172"}]